

fn fibbonaci(a: u32, b: u32, i: u32) -> u32 {
    let fib1: u32;
    let fib2: u32;
    let fib_sum: u32;
    
    fib1 = a;
    fib2 = b;
    while i {
        fib_sum = fib1 + fib2;
        fib1 = fib2;
        fib2 = fib_sum;
        i = i - 1;
    }
    return fib_sum;
}


fn main() -> u32 {
    return fibbonaci(1, 1, 10);
}